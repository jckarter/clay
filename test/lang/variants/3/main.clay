import printer.(println);
import data.vectors.*;

record Human ();

record Dog ();

variant Animal (Human, Dog);

define what;
override what(x:Human) = "Human";
override what(x:Dog) = "Dog";

printPair(a, b) {
    println(what(a), ", ", what(b));
}

main() {
    var v = Vector[Animal]();
    push(v, Animal(Human()));
    push(v, Animal(Dog()));
 
    for (a in v)
        for (b in v)
            printPair(*a, *b);
}
