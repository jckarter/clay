import printer.(println);
record Apple();
record Orange();
record Banana();

variant Fruit (Apple, Orange, Banana);

[F when VariantMember?(Fruit, F) and F != Banana]
Fruit(fruit: F) {
    println("Nope! Bananas!");
    return initializeVariant(Fruit, Banana());
}

main() {
    println(Fruit(Banana()));
    println(Fruit(Apple()));
    println(Fruit(Orange));
}
