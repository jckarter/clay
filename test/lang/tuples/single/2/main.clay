// issue #36

import printer.formatter.(repr);
import printer.(println);

record Thingy[XX] ();

define foo;

default foo(x) { println("not a tuple"); }

[..X] override foo(#Thingy[[..X]]) { println(repr(..X)); }

main() {
    foo(Thingy[[]]);
    foo(Thingy[[Int]]);
    foo(Thingy[[Int, Char]]);

    foo(Thingy[Int]);
}
