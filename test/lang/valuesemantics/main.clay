import printer.(println);
import data.vectors.*;

var _nextId = 0;

nextId() {
    var id = _nextId;
    _nextId +: 1;
    return id;
}

record Test (
    id : Int
);

override RegularRecord?(#Test) = false;

override Test() {
    var id = nextId();
    println("construct Test ", id);
    return Test(id);
}

override Test(src:Test) {
    println("copy Test ", src.id);
    return Test(src.id);
}

override moveUnsafe(src:Test) {
    println("move Test ", src.id);
    return Test(src.id);
}

override resetUnsafe(x:Test) {
    var id = nextId();
    println("reset Test ", id);
    x.id = id;
}

override destroy(x:Test) {
    println("destroy Test ", x.id);
}

override assign(dest:Test, src:Test) {
    dest.id = src.id;
}

main() {
    var a = Vector[Test](Test(), Test());
    reserve(a, 100);
    pushAll(a, array(Test(), Test(), Test(), Test(), Test()));
    var x = Test();
    push(a, move(x));
}
