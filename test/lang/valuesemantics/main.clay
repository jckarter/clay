import printer.(println);
import data.vectors.*;

var _nextId = 0;

nextId() {
    var id = _nextId;
    _nextId +: 1;
    return id;
}

record Test (
    id : Int
);

RegularRecord?(#Test) = false;

Test() {
    var id = nextId();
    println("construct Test ", id);
    return Test(id);
}

Test(src:Test) {
    println("copy Test ", src.id);
    return Test(src.id);
}

moveUnsafe(src:Test) {
    println("move Test ", src.id);
    return Test(src.id);
}

resetUnsafe(x:Test) {
    var id = nextId();
    println("reset Test ", id);
    x.id = id;
}

destroy(x:Test) {
    println("destroy Test ", x.id);
}

assign(dest:Test, src:Test) {
    dest.id = src.id;
}

main() {
    var a = Vector[Test](Test(), Test());
    reserve(a, 100);
    pushAll(a, array(Test(), Test(), Test(), Test(), Test()));
    var x = Test();
    push(a, move(x));
}
