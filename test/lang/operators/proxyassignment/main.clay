import printer.(println);

var global_x = 0;
var global_y = 0;

record Proxy ();

record Point (x:Int, y:Int);

override assign(_:Proxy, p:Point) : {
    global_x = p.x;
    global_y = p.y;
}

override fieldRefAssign(_:Proxy, "x", x:Int) : {
    global_x = x;
}

override fieldRefAssign(_:Proxy, "y", y:Int) : {
    global_y = y;
}

override fieldRef(_:Proxy, "x") : Int = global_x;
override fieldRef(_:Proxy, "y") : Int = global_y;

main() {
    Proxy() = Point(1, 2);
    println(global_x, ", ", global_y);

    Proxy().x = 3;
    println(global_x, ", ", global_y);

    Proxy().y = 4;
    println(global_x, ", ", global_y);
}
