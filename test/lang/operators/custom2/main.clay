import math.(pow);
import printer.(println);

(**)(a,b) = pow(a,b);
(--)(a,b) = pow(a,b);
(***)(a,b) = pow(a,b);
(****)(a,b) = pow(a,b);

override operatorPrecedence(#(**)) = 900;
override operatorAssociativity(#(**)) = LEFT;

override operatorPrecedence(#(--)) = 900;
override operatorAssociativity(#(--)) = LEFT;

override operatorPrecedence(#(***)) = 900;
override operatorAssociativity(#(***)) = RIGHT;

override operatorPrecedence(#(****)) = 880;
override operatorAssociativity(#(****)) = RIGHT;

main() {
    
    println(4 ** 3 ** 2);  //4096
    println(4 ** 3 -- 2);  //4096
    println(4 -- 3 ** 2);  //4096
    println(4 *** 3 *** 2); //262144
    println(4 **** 3 **** 2); //262144

    println(4 ** 3 *** 2); //4096
    println(4 *** 3 ** 2); //262144
    
    println(4 ** 3 **** 2); //262144
    println(4 **** 3 ** 2); //4096
    println(4 **** 3 *** 2); //4096
    println(4 *** 3 **** 2); //262144

}
