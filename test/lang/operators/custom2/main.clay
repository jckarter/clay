import math.(pow);
import printer.(println);

(**)(a,b) = pow(a,b);
(--)(a,b) = pow(a,b);
(***)(a,b) = pow(a,b);
(****)(a,b) = pow(a,b);

operatorPrecedence(#(**)) = 900;
operatorAssociativity(#(**)) = LEFT;

operatorPrecedence(#(--)) = 900;
operatorAssociativity(#(--)) = LEFT;

operatorPrecedence(#(***)) = 900;
operatorAssociativity(#(***)) = RIGHT;

operatorPrecedence(#(****)) = 880;
operatorAssociativity(#(****)) = RIGHT;

main() {
    
    println(4 ** 3 ** 2);  //4096
    println(4 ** 3 -- 2);  //4096
    println(4 -- 3 ** 2);  //4096
    println(4 *** 3 *** 2); //262144
    println(4 **** 3 **** 2); //262144

    println(4 ** 3 *** 2); //4096
    println(4 *** 3 ** 2); //262144
    
    println(4 ** 3 **** 2); //262144
    println(4 **** 3 ** 2); //4096
    println(4 **** 3 *** 2); //4096
    println(4 *** 3 **** 2); //262144

}
