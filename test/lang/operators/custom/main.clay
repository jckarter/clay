import foo;
import foo.((~~));
import printer.(println);

(**)(a, b) { println("**"); }

override (~~)(a:Float, b:Float) { println("~~ Float"); }
override foo.(~~)(a:Double, b:Double) { println("~~ Double"); }

operatorAsStaticArg1(#(~~)) { println("#(~~) 1"); }
operatorAsStaticArg2(#foo.(~~)) { println("#(~~) 2"); }

bar(a, b) = (a \ b) * a \ b;

alias (\*\) = bar;

main() {
    1 ** 2;
    1 ~~ 2;
    1f ~~ 2f;
    1. ~~ 2.;

    (**)(1, 2);
    (~~)(1, 2);
    (~~)(1f, 2f);
    (~~)(1., 2.);

    foo.(~~)(1, 2);
    foo.(~~)(1f, 2f);
    foo.(~~)(1., 2.);

    operatorAsStaticArg1((~~));
    operatorAsStaticArg1(foo.(~~));
    operatorAsStaticArg2((~~));
    operatorAsStaticArg2(foo.(~~));

    var a = 8;
    println(a \*\ 3);
    a \*\: 3;
    println(a);
    -:a;
    println(a);
}
