import printer.(println);

record CustomCompare[T,F] (
    x:T
);

[T,F]
newCustomCompare(x:T, #F) = CustomCompare[T,F](x);

[T,F]
lesser?(a:CustomCompare[T,F], b:CustomCompare[T,F]) = F(a.x, b.x);

min(a, b) {
    if (a <= b) return ref a;
    return ref b;
}

myLesser(a, b) = lesser?(a, b);

main() {
    var a = newCustomCompare(10, myLesser);
    var b = CustomCompare[Int32,myLesser](-20);
    println(min(a, b).x);
    return 0;
}
