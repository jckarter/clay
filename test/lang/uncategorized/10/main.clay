import printer.(println);

define printValue;

override printValue(x:Int32) {
    println("int value = ", x);
}

override printValue(x:UInt32) {
    println("uint value = ", x);
}

override printValue(x:Float64) {
    println("double value = ", x);
}

main() {
    var a = 1 + 2u;
    printValue(a);
    return 0;
}
