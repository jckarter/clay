import printer.(println);
import data.vectors.*;

record Multiplier[N] (
    factor:N
);

[N]
overload call(x:Multiplier[N], y) = x.factor * y;

map(f, a) {
    alias ResultType = Type(f(a[0]));
    var b = Vector[ResultType]();
    for (x in a)
        push(b, f(x));
    return b;
}

main() {
    var a = vector(1, 2, 3);
    var b = map(Multiplier(10), a);
    for (x in b)
        println(x);
}
