import printer.(println);
import vectors.*;

map(f, a) {
    alias ResultType = Type(f(a[0]));
    var b = Vector[ResultType]();
    for (x in a)
        push(b, f(x));
    return b;
}

main() {
    var a = Vector[Int](1, 2, 3, 4, 5, 6);

    var f = (x => x*10);
    var b = map(f, a);

    for (x in b)
        println(x);

    var c = map(x => x*x*x, a);

    for (x in c)
        println(x);
}
