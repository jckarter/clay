import printer.(println);

define IsInt64;
[T] override IsInt64(#T) = false;
override IsInt64(#Int64) = true;

define Foo;
[T] override Foo(#T) = false;
override Foo(#Int64) = true;

f(x) = x;

main() {
    alias T = Int32;
    var x = 0;
    if (IsInt64(T) and Foo(T))
        x +: 1;
    println("x = ", x);
}
