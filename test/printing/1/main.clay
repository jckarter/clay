
enum Gender (
    MALE,
    FEMALE,
);

record Person (
    name : StringLiteralRef,
    age : Int,
    gender: Gender,
    address : Address,
);

record Address (
    email : StringLiteralRef,
);

main() {
    var p = Person(StringLiteralRef("Foo"), 100, FEMALE, Address(StringLiteralRef("foo@bar.com")));
    println(p);
}
