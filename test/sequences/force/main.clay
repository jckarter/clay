import printer.(println);
import deques.*;

triple(x) = 3*x;
even?(x) = (x % 2 == 0);

main() {
    var a = integerArray(#5);
    displayForce(a);

    var b = reversed(mapped(triple, a));
    displayForce(b);

    var c = filtered(even?, b);
    displayForce(c);

    var d = mapped(triple, Deque(c));
    displayForce(d);
}

displayForce(x) {
    alias a = force(x);
    println(Type(a), ": ", Vector(a));
}
