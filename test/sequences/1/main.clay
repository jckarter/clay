import printer.(println);

even?(x) = (x % 2 == 0);

main() {
    var a = Vector[Int](1, 2, 3, 4, 5, 6);
    for (x in filtered(even?, a))
        x *: 10;
    var odd? = (x => x % 2 == 1);
    for (x in filtered(odd?, a))
        x *: 100;
    for (x in filtered(even?, move(a)))
        println(x);
}
