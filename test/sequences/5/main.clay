import printer.(println,print);
import sequences.*;
import vectors.*;

printList(a) {
    print(Type(a), ": ");
    for (i, x in enumerated(a)) {
        if (i != 0)
            print(", ");
        print(x);
    }
    println();
}

main() {
    printList(range(5));
    var a = reversed(range(10));
    printList(a);
    printList(reversed(a));

    var b = array(1, 2, 3, 4, 5);
    printList(reversed(b));

    var c = reversed(Vector(b));
    printList(c);

    var d = Vector(b);
    var e = reversed(d);
    printList(e);

    var f = reversed(e);
    printList(f);
}
