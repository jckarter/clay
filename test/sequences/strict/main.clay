
import deques.*;
import printer.(println);

triple(x) = 3*x;
even?(x) = (x % 2 == 0);

main() {
    var a = integerArray(#5);
    display(a);

    var b = reverse(map(triple, a));
    display(b);

    var c = filter(even?, b);
    display(c);

    var d = map(triple, Deque(c));
    display(d);
}

display(a) {
    println(Type(a), ": ", Vector(a));
}
