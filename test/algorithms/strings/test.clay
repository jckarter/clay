import test.*;

main() = testMain(
    TestSuite(
        "algorithms.strings", Tuple(
            TestCase("trim", test => {
                expectEqual(test, "trim, string",
                    trim("   \t A string,\nthat\tneeds trimming. \n  \n "),
                    "A string,\nthat\tneeds trimming.",
                );
                expectEqual(test, "trim, predicate",
                    trim("*** message ***", x => (x == "*"[0] or x == " "[0])),
                    "message"
                );
            }),
        )
    )
);
