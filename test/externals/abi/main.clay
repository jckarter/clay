external (cdecl) clay_return_22(arg0:Int) : Union22 {
    var returned = Union22();
    switch (arg0)
    case (0)
        returned.0 <-- 0xC1A4C1A4u;
    case (1)
        returned.1 <-- 0x1.C1A4C0p123f;
    else
        libc.abort();
    return returned;
}

