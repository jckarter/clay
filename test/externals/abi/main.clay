clay_to_c() {
    println("\nPassing Clay arguments to C:");
    flush(stdout);

    c_scalar(clay_return_int(), clay_return_bool(), clay_return_float(), clay_return_double());
    c_complex_float(clay_return_complex_float());
    c_complex_double(clay_return_complex_double());
    c_1(clay_return_1());
    c_2(clay_return_2());
    c_3(clay_return_3());
