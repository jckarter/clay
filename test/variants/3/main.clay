
record Human ();

record Dog ();

variant Animal = Human | Dog;

procedure what;
overload what(x:Human) = "Human";
overload what(x:Dog) = "Dog";

printPair(a, b) {
    println(what(a), ", ", what(b));
}

main() {
    var v = Vector[Animal]();
    push(v, Animal(Human()));
    push(v, Animal(Dog()));
 
    for (a in v)
        for (b in v)
            printPair(*a, *b);
}
