
import any.*;

main() {
    var a = Vector[Any]();

    push(a, box(true));
    push(a, box(10));
    push(a, box(a));

    for (x in a) {
        if (anyIs?(x, Bool)) {
            var boolValue = unbox(x, Bool);
            println("Bool ", boolValue);
        }
        else if (anyIs?(x, Int)) {
            var intValue = unbox(x, Int);
            println("Int ", intValue);
        }
        else {
            println("Other");
        }
    }
}
