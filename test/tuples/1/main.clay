
main() {
    println([] == []);
    println([1] == Tuple(1));
    println([1] != Tuple(2));
    println([1, 2] == Tuple(1, 2));
    println([1, 2] != Tuple(1, 3));

    unpackAndShow([]);
    unpackAndShow(1);
    unpackAndShow([1, 2]);
    unpackAndShow([1, 2, 3]);
}

unpackAndShow(x) {
    println("values: ", ..weaveValues(", ", ..unpack(x)));
}
