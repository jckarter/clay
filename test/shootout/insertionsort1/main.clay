
insertionSort(a) {
    var i = 1;
    while (i < size(a)) {
        var x = a[i];
        var j = i;
        while (a[j-1] > x) {
            a[j] = a[j-1];
            j -: 1;
            if (j == 0) break;
        }
        a[j] = x;
        i +: 1;
    }
}

reverse(a) {
    var n = size(a);
    for (i in range(n \ 2))
        swap(a[i], a[n-i-1]);
}

main() {
    var a = Vector[Int]();
    for (i in range(1000))
        push(a, i*2);
    for (i in range(1000)) {
        insertionSort(a);
        reverse(a);
    }
    println("a[0] = ", a[0]);
    return 0;
}
