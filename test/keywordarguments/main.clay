import keywordarguments.*;
import printer.(println);

foo(x: Int, zim: Int, zang: StringLiteralRef, zung: Double) {
    println();
    println("x = ", x);
    println("zim = ", zim);
    println("zang = ", zang);
    println("zung = ", zung);
}

requiredKeywords(x, ..keywords) {
    foo(x, ..keywordArguments(["zim", "zang", "zung"], ..keywords));
}

optionalKeywords(x, ..keywords) {
    foo(x, ..keywordArguments([zim: 5, zang: StringLiteralRef("six"), zung: 7.0], ..keywords));
}

main() {
    requiredKeywords(0, zim: 1, zang: StringLiteralRef("two"), zung: 3.0);
    optionalKeywords(4);
    optionalKeywords(8, zim: 9);
    optionalKeywords(10, zang: StringLiteralRef("eleven"));
    optionalKeywords(12, zung: 13.0);
}
