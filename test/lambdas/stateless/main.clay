
import libc;
import printer.(println);

main() {
    var statelessLambda = () => { println("bye!"); };
    var ptr = makeCCodePointer(statelessLambda);
    libc.atexit(ptr);

    var fs = Vector[CodePointer[[],[]]]();
    ..for (i in staticIntegers(#5)) {
        var f = () => { println(unwrapStatic(i)); };
        push(fs, makeCodePointer(f));
    }

    for (f in fs)
        f();
}
