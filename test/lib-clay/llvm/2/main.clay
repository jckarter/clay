import printer.(println);

define myadd;

[T when Integer?(T)]
override myadd(x:T, y:T) --> returned:T __llvm__{
    %1 = load $T* %x
    %2 = load $T* %y
    %3 = add $T %1, %2
    store $T %3, $T* %returned
    ret i8* null
}


[T when Float?(T)]
override myadd(x:T, y:T) --> returned:T __llvm__{
    %1 = load $T* %x
    %2 = load $T* %y
    %3 = fadd $T %1, %2
    store $T %3, $T* %returned
    ret i8* null
}

main() {
    var a1 = myadd(10, 20);
    var a2 = myadd(20ul, 30ul);
    var a3 = myadd(30l, 40l);
    var a4 = myadd(1.2, 2.4);
    println(a1, " ", Type(a1));
    println(a2, " ", Type(a2));
    println(a3, " ", Type(a3));
    println(a4, " ", Type(a4));
}

