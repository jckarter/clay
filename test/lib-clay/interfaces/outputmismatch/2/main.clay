import printer.(println);

[T] define byval(x:T) : T;
[T] define byref(ref x:T) : ByRef[T];

byval(x:Int) = ref x;
byval(x:Double) = x;

byref(ref x:Int) = ref x;
byref(ref x:Double) = x;

main() {
    println(byval(1));
    println(byval(1.0));

    var i = 1;
    var f = 1.0;

    println(byref(i));
    println(byref(f));
}
