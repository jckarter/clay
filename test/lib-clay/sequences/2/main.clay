import printer.(println);
import sequences.*;
import vectors.*;

double(x) = 2*x;

main() {
    var a = Vector[Int](1, 2, 3, 4, 5, 6);
    var b = mapped(addressOf, a);
    var c = mapped(dereference, b);
    for (x in c)
        x +: 2;
    for (x in mapped(double, mapped(double, a)))
        println(x);
    for (x in mapped(double, range(5)))
        println(x);
}
