import printer.(println);
import strings.*;
import lambdas.*;

record Foo[F] ( fn : F );
var z = Foo(x => { println(x); });

foo(..args) {
    var f = msg => { println(msg, ": ", ..args); };
    f("first");
    f("second");
}

main() {
    z.fn(10);
    foo(10, ", ", 20);

    var g = x => (x, x+1, x+2);
    println(..weaveValues(", ", ..g(99)));
}
