import math.native.(sqrt);
import printer.formatter.(repr);
import printer.(println);
import complex.*;

main() {
    println(repr(sqrt(4.0f)));
    println(repr(sqrt(4.0)));

    println(repr(sqrt(-4.0f+0.0fj)));
    println(repr(sqrt(2.0fj)));
    println(repr(sqrt(-4.0+0.0j)));
    println(repr(sqrt(2.0j)));
}
