import base.*;

overloadable printValue;

overload printValue(x:Int32) {
    println("int value = ", x);
}

overload printValue(x:UInt32) {
    println("uint value = ", x);
}

overload printValue(x:Float64) {
    println("double value = ", x);
}

main() {
    var a = 1 + 2#u32;
    printValue(a);
    return 0;
}
