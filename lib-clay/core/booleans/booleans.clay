
forceinline overload Bool() = false;

[I when Integer?(I)]
forceinline overload I(a:Bool) {
    if (a) return I(1);
    else return I(0);
}

[I when Integer?(I)]
forceinline overload Bool(a:I) = (a != I(0));

forceinline overload equals?(a:Bool, b:Bool) {
    if (a) return b;
    return not b;
}

forceinline overload lesser?(a:Bool, b:Bool) = ((not a) and b);
