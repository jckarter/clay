
forceinline Bool() : Bool = false;

[I when Integer?(I)]
forceinline I(a:Bool) : I = if (a) I(1) else I(0);

[I when Integer?(I)]
forceinline Bool(a:I) : Bool = (a != I(0));

forceinline equals?(a:Bool, b:Bool) : Bool 
	= if (a) b else not b;

forceinline lesser?(a:Bool, b:Bool) : Bool = ((not a) and b);
