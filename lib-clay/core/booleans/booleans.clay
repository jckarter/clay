
forceinline override Bool() : Bool = false;

[I when Integer?(I)]
forceinline override I(a:Bool) : I = if (a) I(1) else I(0);

[I when Integer?(I)]
forceinline override Bool(a:I) : Bool = (a != I(0));

forceinline override equals?(a:Bool, b:Bool) : Bool 
	= if (a) b else not b;

forceinline override lesser?(a:Bool, b:Bool) : Bool = ((not a) and b);
