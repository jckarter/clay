	  MAKE_IEEE(0xCF5C5703u, 0x3FD4DA33u), MAKE_IEEE(0x7BE71FBEu, 0x3C7B1F07u)),
	array(MAKE_IEEE(0x773D5255u, 0x3FF0D92Cu), MAKE_IEEE(0x3D5D1DA2u, 0xBC84956Cu),
