/* the coefficients for the cosh-approximations */
//alias c0 =   ieee_db(array(0x00000000,0x3FF00000)).2;     /*1.0000000000e+00*/
alias c2 =  MAKE_IEEE(0x3FE00000,0x00000000).2;     /*5.0000000000e-01*/
alias c4 =  MAKE_IEEE(0x3FA55555,0x55555555).2;     /*4.1666666667e-02*/
alias c6 =  MAKE_IEEE(0x3F56C16C,0x16C16C17).2;     /*1.3888888889e-03*/
alias c8 =  MAKE_IEEE(0x3EFA01A0,0x1A01A01A).2;     /*2.4801587302e-05*/
/* the coefficients for the sinh-approximations */
//alias s1 =  MAKE_IEEE(0x3FF00000,0x00000000).2;     /*1.0000000000e+00*/
alias s3 =  MAKE_IEEE(0x3FC55555,0x55555555).2;     /*1.6666666667e-01*/
alias s5 =  MAKE_IEEE(0x3F811111,0x11111111).2;     /*8.3333333333e-03*/
alias s7 =  MAKE_IEEE(0x3F2A01A0,0x1A01A01A).2;     /*1.9841269841e-04*/

private do_cosh(x:Double) --> preshi:Double, preslo:Double {
  
  	var b_hi, b_lo = 0.,0.;
  	/* First range reduction*/
  	var k = DOUBLE2INT(x * IVLN2);
    if (k != 0){ /* b_hi+b_lo =  x - (LN2HI + ln2_lo) * k */
		var temp_hi = x - LN2HI * k;
		var temp_lo = -ln2_lo * k;
		b_hi, b_lo = Add12Cond( temp_hi, temp_lo);
    }
    else {
      	b_hi = x;  
      	b_lo = 0.;
    }
	/*we'll construct 2 constants for the last reconstruction */
	var two_p_plus_k = MAKE_IEEE(bitshl(k-1+1023 , 20), 0);
	var two_p_minus_k = MAKE_IEEE(bitshl(-k-1+1023 , 20), 0);
	
	/* at this stage, we've done the first range reduction : we have b_hi + b_lo  between -ln(2)/2 and ln(2)/2 */
	/* now we can do the second range reduction */
	/* we'll get the 8 leading bits of b_hi */
	var table_index_float = MAKE_IEEE(b_hi + two_43_44);
	/*this add do the float equivalent of a rotation to the right, since -0.5 <= b_hi <= 0.5*/
	var table_index = GET_IEEE_LOW(table_index_float);/* -89 <= table_index <= 89 */
	GET_IEEE_DB(table_index_float) -= two_43_44;
	table_index += bias; /* to have only positive values */
	b_hi -= GET_IEEE_DB(table_index_float);/* to remove the 8 leading bits*/
	/* since b_hi was between -2!-1 and 2!1, we now have b_hi between -2!-9 and 2!-9 */

  	var y = MAKE_IEEE(b_hi);
	/*   first, y  */
	var square_b_hi = b_hi * b_hi;
	/* effective computation of the polynomial approximation */
	var tcb_hi,tsb_hi = 0.,0.;
	if (bitand(GET_IEEE_HIGH(y),0x7FFFFFFF) >= GET_IEEE_HIGH(two_minus_30)) {
		/*   second, cosh(y) = y * (1/2 + y * (1/24 + y * 1/720)) */
		tcb_hi = (square_b_hi)* (c2 + square_b_hi * (c4 + square_b_hi * c6));
		tsb_hi = square_b_hi * (s3 + square_b_hi * (s5 + square_b_hi * s7));
	}

	var ch_hi, ch_lo = 0.,0.;
	if( table_index != bias) {
		/* we get the tabulated the tabulated values */
		ref ca_hi = cosh_sinh_table[table_index][0].2;
		ref ca_lo = cosh_sinh_table[table_index][1].2;
		ref sa_hi = cosh_sinh_table[table_index][2].2;
		ref sa_lo = cosh_sinh_table[table_index][3].2;

		/* first reconstruction of the cosh (corresponding to the second range reduction) */
		var b_sa_hi,b_sa_lo = Mul12( sa_hi, b_hi);
		var temp = ((((((ca_lo + (b_hi * sa_lo)) + b_lo * sa_hi) + b_sa_lo) + (b_sa_hi * tsb_hi)) + ca_hi * tcb_hi) + b_sa_hi);
		ch_hi, ch_lo = Add12Cond( ca_hi, temp);
		  /* first reconstruction for the sinh (corresponding to the second range reduction) */
	}
  	else {
    	ch_hi, ch_lo = Add12Cond( 1., tcb_hi);
  	}

	if(k != 0) {
  		var sh_hi, sh_lo = 0.,0.;
	    if( table_index != bias) {
			ref ca_hi = cosh_sinh_table[table_index][0].2;
			ref ca_lo = cosh_sinh_table[table_index][1].2;
			ref sa_hi = cosh_sinh_table[table_index][2].2;
			ref sa_lo = cosh_sinh_table[table_index][3].2;
			/* first reconstruction for the sinh (corresponding to the second range reduction) */
			var b_ca_hi , b_ca_lo = Mul12( ca_hi, b_hi);
			var temp = (((((sa_lo + (b_lo * ca_hi)) + (b_hi * ca_lo)) + b_ca_lo) + (sa_hi*tcb_hi)) + (b_ca_hi * tsb_hi));
			var temp_hi, temp_lo = Add12( b_ca_hi, temp);
			sh_hi, sh_lo = Add22Cond( sa_hi,0., temp_hi, temp_lo);
	    }
    	else {
      		sh_hi, sh_lo = Add12Cond( b_hi, tsb_hi * b_hi + b_lo);
    	}
	    if(35 > k > -35) {
			var ch_2_pk_hi = ch_hi * GET_IEEE_DB(two_p_plus_k);
			var ch_2_pk_lo = ch_lo * GET_IEEE_DB(two_p_plus_k);
			var ch_2_mk_hi = ch_hi * GET_IEEE_DB(two_p_minus_k);
			var ch_2_mk_lo = ch_lo * GET_IEEE_DB(two_p_minus_k);
			var sh_2_pk_hi = sh_hi * GET_IEEE_DB(two_p_plus_k);
			var sh_2_pk_lo = sh_lo * GET_IEEE_DB(two_p_plus_k);
			var sh_2_mk_hi = - sh_hi * GET_IEEE_DB(two_p_minus_k);
			var sh_2_mk_lo = - sh_lo * GET_IEEE_DB(two_p_minus_k);

			preshi, preslo <-- Add22Cond( ch_2_mk_hi, ch_2_mk_lo, sh_2_mk_hi, sh_2_mk_lo);
			ch_2_mk_hi, ch_2_mk_lo  = Add22Cond(sh_2_pk_hi, sh_2_pk_lo, preshi, preslo);
			preshi, preslo = Add22Cond( ch_2_pk_hi, ch_2_pk_lo, ch_2_mk_hi, ch_2_mk_lo);
	    }
	    else if (k >= 35) {
			var ch_2_pk_hi = ch_hi * GET_IEEE_DB(two_p_plus_k);
			var ch_2_pk_lo = ch_lo * GET_IEEE_DB(two_p_plus_k);
			var sh_2_pk_hi = sh_hi * GET_IEEE_DB(two_p_plus_k);
			var sh_2_pk_lo = sh_lo * GET_IEEE_DB(two_p_plus_k);
			preshi, preslo <-- Add22Cond( ch_2_pk_hi, ch_2_pk_lo, sh_2_pk_hi, sh_2_pk_lo);
	    }
	    else {/* if (k <= -35) */
		    var ch_2_mk_hi = ch_hi * GET_IEEE_DB(two_p_minus_k);
			var ch_2_mk_lo = ch_lo * GET_IEEE_DB(two_p_minus_k);
			var sh_2_mk_hi = - sh_hi * GET_IEEE_DB(two_p_minus_k);
			var sh_2_mk_lo = - sh_lo * GET_IEEE_DB(two_p_minus_k);
			preshi, preslo <-- Add22Cond( ch_2_mk_hi, ch_2_mk_lo, sh_2_mk_hi, sh_2_mk_lo);
	    }
  	}
  	else {
    	preshi,preslo <-- ch_hi,ch_lo;
  	}
}

private do_cosh_accurate(x) 
	--> pexponent:Int,presh:Double,presm:Double,presl:Double {
	
	var expmh, expmm, expml = MAKE_IEEE(0.),MAKE_IEEE(0.),MAKE_IEEE(0.);

	if(x<0) x = -x;
	if (x > 40.0) {  /* then exp(-x) < 2!-118 exp(x) */
		presh, presm, presl,pexponent <-- exp13(x);
	}
	else {
		var expph, exppm, exppl = 0.,0.,0.;
		expph, exppm, exppl, pexponent <-- exp13(x);
		var exponentm = 0;
		expmh.2, expmm.2, expml.2, exponentm = exp13(-x);
		/* align the mantissas.
		The exponent is increased but stays well below overflow threshold */
		var deltaexponent =  exponentm - pexponent ;
		GET_IEEE_HIGH(expmh) += bitshl(deltaexponent,20);
		GET_IEEE_HIGH(expmm) += bitshl(deltaexponent,20);
		GET_IEEE_HIGH(expml) += bitshl(deltaexponent,20);
		var exph, expm, expl = Add33(expph,exppm,exppl,expmh.2, expmm.2, expml.2);
		presh,presm,presl <-- Renormalize3(exph, expm, expl);
	}
}

overload cosh(x:Double){
  
  var y = MAKE_IEEE(x);
  var hx = bitand(GET_IEEE_HIGH(y),0x7FFFFFFF);

  /* Filter special cases */
  if (hx > GET_IEEE_HIGH(max_input_csh)) { /* strictly greater, implies x > max_input_csh */
    if (hx >= 0x7ff00000){  /* Infty or NaN */
      if (bitor(bitand(hx,0x000fffff),GET_IEEE_LOW(y))!=0)
		return x+x;                /* Nan */
      else {/* otherwise the result should be +infty */
		GET_IEEE_HIGH(y) = 0x7FF00000;
		return GET_IEEE_DB(y);
      }
    }
  }
  if (x >= GET_IEEE_DB(max_input_csh) or x <= -GET_IEEE_DB(max_input_csh))
    return largest_double * largest_double;     /* overflow  */
  if (hx<0x3e500000) {
    if(x==0)
      return 1.0; /* exact */
    else
      return (1.0+tiniest_double); /* to raise inexact flag */
  }

  var rh, rl = do_cosh(x);


  if (rh == (rh + (rl * round_cst_csh))) return rh;
  else{
    var exponent,resh,resm,resl = do_cosh_accurate(x);
    var res = MAKE_IEEE(RoundToNearest3( resh, resm, resl));

    /* Now we have to set the exponent of res as exponent -1 (division
       by 2). However, as res may sometimes end up infinite, we first
       set the exponent to exponent -11 and then multiply by 2!10,
       which will cater for overflow  */
    GET_IEEE_HIGH(res) += bitshl(exponent-11,20);
    return 1024. * GET_IEEE_DB(res);
  }
}

private do_sinh(x:Double) --> prh:Double, prl:Double {

  var b_hi, b_lo = 0.,0.;
  /* Now we can do the first range reduction*/
  var k = DOUBLE2INT(x * IVLN2);
    if (k != 0){ /* b_hi + b_lo =  x - (LN2HI + ln2_lo) * k */
      var temp_hi = x - LN2HI * k;
      var temp_lo = -ln2_lo * k;
      b_hi, b_lo = Add12Cond( temp_hi, temp_lo);
    }
    else {
      b_hi = x;  
      b_lo = 0.;
    }

  /*we'll construct 2 constants for the last reconstruction */
  var two_p_plus_k = MAKE_IEEE(bitshl(k-1+1023,20), 0);
  var two_p_minus_k = MAKE_IEEE(bitshl(-k-1+1023,20), 0);

  /* at this stage, we've done the first range reduction : we have b_hi + b_lo  between -ln(2)/2 and ln(2)/2 */
  /* now we can do the second range reduction */
  /* we'll get the 8 leading bits of r_hi */

  var table_index_float = MAKE_IEEE(b_hi + two_43_44);
  /*this add do the float equivalent of a rotation to the right, since -0.5 <= b_hi <= 0.5*/
  var table_index = GET_IEEE_LOW(table_index_float);/* -89 <= table_index <= 89 */
  GET_IEEE_DB(table_index_float) -= two_43_44;
  table_index += bias; /* to have only positive values */
  b_hi -= GET_IEEE_DB(table_index_float);/* to remove the 8 leading bits*/
  /* since b_hi was between -2!-1 and 2!1, we now have b_hi between -2!-9 and 2!-9 */

  var y = MAKE_IEEE(b_hi);
  /*   first, y = square_y_hi + square_y_lo  */
  var square_y_hi = b_hi * b_hi;
  var tsb_hi,tcb_hi = 0.,0.;
  /* effective computation of the polyomial approximation */
  if (bitand(GET_IEEE_HIGH(y),0x7FFFFFFF) > GET_IEEE_HIGH(two_minus_30))  {
    tsb_hi = square_y_hi * (s3 + square_y_hi * (s5 + square_y_hi * s7));
    /*   second, cosh(y) = y * (1/2 + y * (1/24 + y * 1/720)) */
    tcb_hi = (square_y_hi)* (c2 + square_y_hi * (c4 + square_y_hi * c6));
  }
  var sh_hi, sh_lo,ch_hi, ch_lo = 0.,0.,0.,0.;
  if( table_index != bias) {
    /* we get the tabulated the tabulated values*/
    ref ca_hi = cosh_sinh_table[table_index][0].2;
    ref ca_lo = cosh_sinh_table[table_index][1].2;
    ref sa_hi = cosh_sinh_table[table_index][2].2;
    ref sa_lo = cosh_sinh_table[table_index][3].2;

    /* first reconstruction for the sinh (corresponding to the second range reduction) */
    var temp1 = sa_lo;
    temp1 += b_lo * ca_hi;
    temp1 += b_hi * ca_lo;
    var ca_b_hi, ca_b_lo = Mul12(ca_hi, b_hi);
    temp1 += ca_b_lo;
    temp1 += sa_hi * tcb_hi;
    temp1 += ca_b_hi * tsb_hi;
    var temp_hi, temp_lo = Add12Cond( ca_b_hi, temp1);
    sh_hi, sh_lo = Add22Cond(sa_hi, 0., temp_hi, temp_lo);
    /* first reconstruction of the cosh (corresponding to the second range reduction) */
    temp1 = ca_lo;
    var sa_b_hi,sa_b_lo = Mul12(sa_hi, b_hi);
    temp1 += b_hi * sa_lo;
    temp1 += b_lo * sa_hi;
    temp1 += sa_b_lo;
    temp1 += sa_b_hi * tsb_hi;
    temp1 += ca_hi * tcb_hi;
    temp1 += sa_b_hi;
    ch_hi, ch_lo = Add12Cond( ca_hi, temp1);
  }
  else {
    sh_hi, sh_lo = Add12Cond(b_hi, tsb_hi * b_hi + b_lo);
    ch_hi, ch_lo = Add12Cond(1. , tcb_hi);
  }

  if(k != 0) {
    if( 35 > k > -35) {
		var ch_2_pk_hi = ch_hi * GET_IEEE_DB(two_p_plus_k);
		var ch_2_pk_lo = ch_lo * GET_IEEE_DB(two_p_plus_k);
		var ch_2_mk_hi = - ch_hi * GET_IEEE_DB(two_p_minus_k);
		var ch_2_mk_lo = - ch_lo * GET_IEEE_DB(two_p_minus_k);
		var sh_2_pk_hi = sh_hi * GET_IEEE_DB(two_p_plus_k);
		var sh_2_pk_lo = sh_lo * GET_IEEE_DB(two_p_plus_k);
		var sh_2_mk_hi = sh_hi * GET_IEEE_DB(two_p_minus_k);
		var sh_2_mk_lo = sh_lo * GET_IEEE_DB(two_p_minus_k);

		prh, prl <-- Add22Cond(ch_2_mk_hi, ch_2_mk_lo, sh_2_mk_hi, sh_2_mk_lo);
		ch_2_mk_hi, ch_2_mk_lo = Add22Cond( sh_2_pk_hi, sh_2_pk_lo, prh, prl);
		prh, prl = Add22Cond( ch_2_pk_hi, ch_2_pk_lo, ch_2_mk_hi, ch_2_mk_lo);
    }
    else if (k >= 35) {
		var ch_2_pk_hi = ch_hi * GET_IEEE_DB(two_p_plus_k);
		var ch_2_pk_lo = ch_lo * GET_IEEE_DB(two_p_plus_k);
		var sh_2_pk_hi = sh_hi * GET_IEEE_DB(two_p_plus_k);
		var sh_2_pk_lo = sh_lo * GET_IEEE_DB(two_p_plus_k);
		prh, prl <-- Add22Cond(ch_2_pk_hi, ch_2_pk_lo, sh_2_pk_hi, sh_2_pk_lo);
    }
    else {
		var ch_2_mk_hi = - ch_hi * GET_IEEE_DB(two_p_minus_k);
		var ch_2_mk_lo = - ch_lo * GET_IEEE_DB(two_p_minus_k);
		var sh_2_mk_hi = sh_hi * GET_IEEE_DB(two_p_minus_k);
		var sh_2_mk_lo = sh_lo * GET_IEEE_DB(two_p_minus_k);
		prh, prl <-- Add22Cond( ch_2_mk_hi, ch_2_mk_lo, sh_2_mk_hi, sh_2_mk_lo);
    }
  }
  else {
    prh,prl <-- sh_hi,sh_lo;
  }
}





private do_sinh_accurate(x:Double) 
	--> pexponent:Int,presh:Double,presm:Double,presl:Double {
  
  if(x > 40.0) { /* then exp(-x) < 2!-129 exp(x) */
    presh, presm, presl, pexponent <-- exp13( x);
  }
  else if(x < -40.0) { /* then exp(x) < 2!-129 exp(-x) */
    presh, presm, presl, pexponent <-- exp13(-x);
    presh = -presh;
    presm = -presm;
    presl = -presl;
  }
  /* Otherwise we are between -40 and 40, and we also know that |x| > 2!-25 */
  else if(x>0.0) {
    var expph, exppm, exppl = expm1_13( x);
    var expmh, expmm, expml = expm1_13(-x);
    /* The following is OK because expph and  -expmh have the same sign */
    var exph, expm, expl = Add33(expph, exppm, exppl,   -expmh, -expmm, -expml);
    presh,presm,presl <-- Renormalize3( exph, expm, expl);
    pexponent <-- 0;
  }
  else  { /* x<0 */
    var expph, exppm, exppl = expm1_13( x);
    var expmh, expmm, expml = expm1_13(-x);
    /* The following is OK because expph and  -expmh have the same sign */
    var exph, expm, expl = Add33( -expmh, -expmm, -expml, expph, exppm, exppl);
    presh,presm,presl <-- Renormalize3(exph, expm, expl);
    pexponent <-- 0;
  }
}


overload sinh(x:Double){
    var y = MAKE_IEEE(x);
    var hx = bitand(GET_IEEE_HIGH(y),0x7FFFFFFF);

    /* Filter special cases */
    if (hx > GET_IEEE_HIGH(max_input_csh)) { /* strictly greater, implies x > max_input_csh */
        if (hx >= 0x7ff00000){ /* infinity or NaN */
            if (bitor(bitand(hx,0x000fffff),GET_IEEE_LOW(y))!=0)
                return x+x;                                        /* NaN */
            else /* otherwise the result should be +infty */
                return GET_IEEE_DB(y);
        }
        if (x > GET_IEEE_DB(max_input_csh))
            return largest_double * largest_double;     /* overflow  */
        if (x < -GET_IEEE_DB(max_input_csh))
            return -largest_double * largest_double;     /* overflow  */
    }

    if (hx < 0x3e500000)
        return x; /* exact, we should find some way of raising the inexact flag */

    var rh, rl = do_sinh(x);

    if (rh == (rh + (rl * round_cst_csh)))
        return rh;
    else{
        var exponent, resh, resm, resl = do_sinh_accurate(x);
        var res = MAKE_IEEE(RoundToNearest3(resh, resm, resl));

        /* Now we have to set the exponent of res as exponent -1 (division
        by 2). However, as res may sometimes end up infinite, we first
        set the exponent to exponent -11 and then multiply by 2!10,
        which will cater for overflow  */
        GET_IEEE_HIGH(res) += bitshl(exponent-11, 20);
        return 1024. * GET_IEEE_DB(res);
    }
}




