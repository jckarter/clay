	  MAKE_IEEE(0xF66C8331u, 0x3FD5A481u), MAKE_IEEE(0xCFF78DD1u, 0x3C6E8E0Cu)),
	array(MAKE_IEEE(0x8AFDD406u, 0x3FF0E94Eu), MAKE_IEEE(0xB0B14F49u, 0xBC9330CBu),
	  MAKE_IEEE(0x47CFA1DBu, 0x3FD5E81Cu), MAKE_IEEE(0x4E7B2C63u, 0xBC791EC5u)),
	array(MAKE_IEEE(0x07A16DB4u, 0x3FF0EED1u), MAKE_IEEE(0xD3E071F5u, 0x3C988108u),
