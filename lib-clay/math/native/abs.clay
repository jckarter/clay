
import numbers.floats.(imagFromBits,floatFromBits,floatBitsUnsigned);
import math.native.protocol.*;

[I when Integer?(I) ]
forceinline abs(x:I) = if(x >= 0) x else -x;

[T when Imaginary?(T) ]
forceinline abs(x:T) = imagFromBits(floatBitsUnsigned(x));
[T when Float?(T) ]
forceinline abs(x:T) = floatFromBits(floatBitsUnsigned(x));

