import data.sequences.lazy.zipped.*;

define enumerated;
define enumeratedAs;

override enumerated(ref a) = zipped(range(size(a)), a);

override enumerated(rvalue a) {
    var n = size(a);
    return zipped(range(n), move(a));
}

override enumeratedAs(T, ref a) = zipped(range(T(size(a))), a);

override enumeratedAs(T, rvalue a) {
    var n = T(size(a));
    return zipped(range(n), move(a));
}
